{% assign menu = linklists['main-menu'] %} {# Replace 'main-menu' with your actual menu handle #}

<ul>
  {% for link in menu.links %}
    {% if link.type == 'collection_link' %}
      <li>{{ link.object.title }}</li> {# Real collection title #}
    {% else %}
      <li>{{ link.title }}</li> {# Fallback to menu label #}
    {% endif %}
  {% endfor %}
</ul>

