{% schema %}
  {
    "name": "Image Column with Txt",
    "settings": [
      {
      "type": "text",
      "label": "Header",
      "id": "id_header"
      },
      {
      "type": "text",
      "label": "Description",
      "id": "id_desc"
      },
      {
      "type": "text",
      "label": "Button Txt",
      "id": "id_btn"
      },
      {
        "type": "image_picker",
        "label": "Image banner for desktop",
        "id": "id_img1"
      },
      {
        "type": "image_picker",
        "label": "Image banner for mobile",
        "id": "id_img2"
      },
    ],
    "blocks": [
    ],
    "presets": [
      {
        "name": "Image Column with Txt"
      }
    ]
  }
{% endschema %}